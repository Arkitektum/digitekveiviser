
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="~/Content/bower_components/dibk-fb-webcommon/assets/images/favicon.ico" type="image/x-icon" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans" rel="stylesheet">
    <title>@ViewBag.Title - DIBK DigiTEK</title>
    @Styles.Render("~/Content/bower_components/dibk-fb-webcommon/assets/css/styles")
</head>
<body>
    <section class="header-section">
        <div class="header-background-image"></div>

        <div>
            <div class="container">
                <div class="navigation">

                    <a href="/">
                        <img class="mobile-logo" src="~/Content/bower_components/dibk-fb-webcommon/assets/images/dibk_logo_mob.svg">
                        <img class="desktop-logo" src="~/Content/bower_components/dibk-fb-webcommon/assets/images/DiBk_logo_rgb.svg">
                    </a>
                    <nav id="main-menu" class="navigaton-bar">
                        <ul>

                            <li><a href='@Url.Action("Index", "Home")'>DigiTEK</a></li>
                            <li><a href='@Url.Action("brann", "Home")'>Brann</a></li>


                        </ul>
                    </nav>
                    <i id="menu-toggle" class="fa fa-bars" aria-hidden="true"></i>
                </div>
            </div>
        </div>
    </section>

    

    <section>
        <div id="vue-container" class="container general-container">
            @RenderBody()
        </div>
    </section>


    <section>
